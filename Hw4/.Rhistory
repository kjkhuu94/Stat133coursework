haversine <- function(lat1, lon1, lat2, lon2, Radius=6371){ # in km
lat1 <- pi*lat1/180; lat2 <- pi*lat2/180;
lon1 <- pi*lon1/180; lon2 <- pi*lon2/180;
dlon <- lon2 - lon1
dlat <- lat2 - lat1
a <- (sin(dlat/2))^2 + cos(lat1) * cos(lat2) * (sin(dlon/2))^2
c <- 2 * atan2( sqrt(a), sqrt(1-a) )
Radius * c
}
